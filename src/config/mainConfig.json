{
    "topology": {
        "bitLength"  : 160,
        "k"          : 20,
        "alpha"      : 3,
        "bucketStore": {
            "databasePath": "db"
        }
    },
    "net"     : {
        "allowHalfOpenSockets"     : false,
        "connectionRetrySeconds"   : 4,
        "idleConnectionKillTimeout": 30,
        "outboundConnectionTimeout": 2000,
        "myOpenPorts"              : [30415]
    },
    "protocol": {
        "waitForNodeReactionInSeconds": 2,
        "messages"                    : {
            "maxByteLengthPerMessage"      : 1048576,
            "msToKeepNonAddressableMemory" : 10000,
            "msToWaitForIncomingMessage"   : 10000,
            "maxSecondsToWaitForConnection": 4
        },
        "proxy"                       : {
            "maxNumberOfProxies"                   : 10,
            "proxyForMaxNumberOfNodes"             : 2,
            "maxUnsuccessfulProxyTries"            : 4,
            "unsuccessfulProxyTryWaitTimeInSeconds": 30
        },
        "pingpong"                    : {
            "maxWaitingListSize": 20
        },
        "findClosestNodes"            : {
            "cycleExpirationInSeconds" : 2,
            "parallelismDelayInSeconds": 3
        }
    },
    "plugin"  : {
        "folderPath"              : "plugin",
        "pluginConfigName"        : "plugin.json",
        "pluginManagerStateConfig": "pluginManager.json"
    },
    "app"     : {
        "dataPath": "dataPathFolder"
    },
    "search"  : {
        "host"             : "localhost",
        "port"             : 9200,
        "binaryPath"       : "core/search/elasticsearch",
        "databasePath"     : "db/searchStore",
        "searchStoreConfig": "./config/searchStore.json",
        "indexManager": {
            "indexRunnerDelayInMilliSeconds": 10000,
            "indexRunnersInParallel": 3
        }
    },
    "fs"      : {
        "folderWatcher": {
            "interval"      : 1000,
            "binaryInterval": 5000,
            "eventDelay"    : 1000
        }
    }
}